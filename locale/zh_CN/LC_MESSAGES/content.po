msgid "Agent interface"
msgstr "服务人员界面"

#: ../../content/ticket-forms.rst:10
msgid "New phone ticket"
msgstr "创建电话工单"

#: ../../content/ticket-forms.rst:11
msgid "New email ticket"
msgstr "创建邮件工单"

#: ../../content/ticket-forms.rst:13
msgid "External interface"
msgstr "外部人员界面"

#: ../../content/ticket-forms.rst:15
